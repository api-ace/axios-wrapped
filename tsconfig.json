{
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "sourceMap": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ESNext",
    "module": "CommonJS",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "incremental": true,
    "lib": ["ESNext"],
    "baseUrl": "./",  
   
  },
  "exclude": ["node_modules", "tmp", "dist","test"]
}
